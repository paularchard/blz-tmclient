  "scripts": {
    "setup-daemon": "cp -R ./test-daemon/configs/. ./test-daemon/daemon-build/output/",
    "test-node": "mocha \"`git rev-parse --show-toplevel`/src/**/*.test.js\" --exit --timeout 8000",
